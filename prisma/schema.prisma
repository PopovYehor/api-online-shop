generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type CatalogsRating {
  count Int
  /// Multiple data types found: Float: 85.7%, Int: 14.3% out of 21 sampled entries
  rate  Json
}

type OrdersCart {
  category    String
  count       Int
  description String
  id          String
  image       String
  price       Int
  rating      OrdersCartRating
  summPrice   Int
  title       String
  v           Int?
}

type OrdersCartRating {
  count Int
  rate  Int
}

model catalogs {
  id          String         @id @default(auto()) @map("_id") @db.ObjectId
  v           Int?           @map("__v")
  category    String
  description String
  image       String
  /// Multiple data types found: Float: 52.4%, Int: 47.6% out of 21 sampled entries
  price       Json
  rating      CatalogsRating
  title       String
}

model orders {
  id             String       @id @default(auto()) @map("_id") @db.ObjectId
  cart           OrdersCart[]
  createAt       String
  deliveryAddres String
  deliveryType   String
  email          String
  name           String
  paymentType    String
  phone          String
  updateAt       String
}

model users {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String @unique
  password String
}
